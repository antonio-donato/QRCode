        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QRCode"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Genera QR Code" HorizontalAlignment="Left" Margin="10,83,0,0" VerticalAlignment="Top" Width="165" Click="button_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="10,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="497"/>
        <Image x:Name="immagine" HorizontalAlignment="Left" Height="100" Margin="384,143,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
